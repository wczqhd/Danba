/* components/progress.wxss */
.swiper{
  width: 100%;
  height: 6px;
  background: #d2d2d2;
  margin: 20px 0;
  position: relative;
  border-radius: 6px;
}
.line{
  height: 6px;
  background: #dd5866;
  position: absolute;
  left: 0;
  top: 0;
  border-radius: 6px 0 0 6px;
}
.slider{
  border-radius: 50%;
  background: #33c9d4;
  position: absolute;
}